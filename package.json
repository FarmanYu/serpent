{
  "name": "serpent",
  "version": "0.0.0",
  "description": "a mono repository for serpent",
  "private": true,
  "keywords": [
    "serpent"
  ],
  "scripts": {
    "sync": "node scripts/sync_versions",
    "bootstrap:1": "cd projects/serpent-cli && npm install && cd ../..",
    "bootstrap:2": "cd projects/serpent-dev-kits && npm install && cd ../..",
    "bootstrap:3": "cd projects/serpent-react-kits && npm install && cd ../..",
    "bootstrap:4": "lerna bootstrap --ignore @serpent/cli --ignore @serpent/dev-kits --ignore @serpent/react-kits",
    "bootstrap": "npm run bootstrap:1 && npm run bootstrap:2 && npm run bootstrap:3 && npm run bootstrap:4",
    "test": "lerna run test --ignore @serpent/cli",
    "postinstall": "npm run bootstrap"
  },
  "license": "GPL-2.0",
  "author": "Mora <qiuzhongleiabc@126.com> (https://github.com/qiu8310)",
  "repository": {
    "type": "git",
    "url": "https://github.com/qiu8310/serpent"
  },
  "devDependencies": {
    "dot-template-types": "0.0.9",
    "lerna": "^3.13.4"
  }
}
